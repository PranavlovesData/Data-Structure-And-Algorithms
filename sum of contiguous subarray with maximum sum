# User function Template for python3

class Solution:
   
    # Function to find the sum of contiguous subarray with maximum sum.
    def maxSubArraySum(self, arr, N):
        max_sum = -1000000000
        for i in range(N):
            sum = 0
            for j in range(i+1):
                sum += arr[j]
                #print("i",i)
            #print("sum",sum)
            if sum > max_sum:
                max_sum = sum
        return max_sum


# {
# Driver Code Starts
# Initial Template for Python 3

import math



def main():
    T = int(input())
    while (T > 0):
        n = int(input())

        arr = [int(x) for x in input().strip().split()]

        ob = Solution()

        print(ob.maxSubArraySum(arr, n))

        T -= 1


if __name__ == "__main__":
    main()
# } Driver Code Ends
